{
  "name": "kommoney",
  "description": "Web application to manage your personal finances",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "build:1-migrate": "prisma migrate deploy",
    "build:2-build": "next build",
    "build": "run-s build:*",
    "db-up": "docker-compose up -d",
    "db-seed": "prisma db seed",
    "db-migrate-dev": "yarn prisma migrate dev",
    "db-nuke": "docker-compose down --volumes --remove-orphans",
    "dx": "cross-env DATABASE_URL=postgresql://postgres:secret@localhost:5632/prismadb run-s db-up db-migrate-dev db-seed dev",
    "dev": "next dev",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.7.1",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.5",
    "@mui/material": "^5.2.5",
    "@next-auth/prisma-adapter": "1.0.1",
    "@prisma/client": "3.7.0",
    "next": "^12.0.7",
    "next-auth": "4.0.6",
    "nodemailer": "^6.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.22.5"
  },
  "devDependencies": {
    "@types/next-auth": "3.13.0",
    "@types/node": "16.11.17",
    "@types/react": "^17.0.38",
    "cross-env": "^7.0.3",
    "eslint": "^8.5.0",
    "eslint-config-next": "^12.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "prisma": "3.7.0",
    "ts-node": "10.4.0",
    "typescript": "^4.5.4"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "prettier": {
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
